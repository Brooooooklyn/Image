{
  "name": "@napi-rs/image",
  "version": "1.1.1",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "Image processing library",
  "author": {
    "email": "github@lyn.one",
    "name": "LongYinan",
    "url": "https://lyn.one"
  },
  "keywords": [
    "image",
    "lossless",
    "compression",
    "jpeg",
    "jpg",
    "png"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
